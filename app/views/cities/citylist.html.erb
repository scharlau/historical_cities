<p id="notice"><%= notice %></p>

<h1>A List of Cities</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
     
      <th>Year</th>
      
      <th colspan="1"></th>
    </tr>
  </thead>
<tbody>
    <% @cities.each do |city| %>
      <tr>
        <td><%= city.name %></td>
         <td><%= city.year %></td>
         <td><%= city.population %></td>
    
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>


